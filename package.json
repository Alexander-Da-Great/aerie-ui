{
  "name": "aerie-ui",
  "version": "0.9.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rm -rf build && svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "cloc": "cloc src",
    "dev": "npm run version && MODE=development svelte-kit dev",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "pre-commit": "node ./scripts/pre-commit.js",
    "prebuild": "npm run version",
    "preview": "svelte-kit preview",
    "version": "node ./scripts/version.js"
  },
  "devDependencies": {
    "@codemirror/basic-setup": "^0.19.0",
    "@codemirror/lang-json": "^0.19.1",
    "@gov.nasa.jpl.aerie/cam": "^1.0.0",
    "@gov.nasa.jpl.stellar/icons": "^1.0.0",
    "@gov.nasa.jpl.stellar/stellar": "^1.0.7",
    "@sveltejs/adapter-node": "1.0.0-next.55",
    "@sveltejs/kit": "1.0.0-next.187",
    "@types/cookie": "^0.4.1",
    "@types/d3-axis": "^2.1.0",
    "@types/d3-brush": "^2.1.0",
    "@types/d3-drag": "^2.0.0",
    "@types/d3-quadtree": "^2.0.0",
    "@types/d3-scale": "^3.3.0",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-shape": "^2.1.0",
    "@types/pg": "^8.6.1",
    "@types/toastify-js": "^1.9.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "ajv": "^8.6.3",
    "bootstrap-icons": "^1.6.1",
    "cloc": "^2.8.0",
    "cookie": "^0.4.1",
    "d3-axis": "^2.1.0",
    "d3-brush": "^2.1.0",
    "d3-drag": "^2.0.0",
    "d3-quadtree": "^2.0.0",
    "d3-scale": "^3.3.0",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "nanoid": "^3.1.30",
    "pg": "^8.7.1",
    "postgres-interval": "^4.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "split.js": "^1.6.4",
    "svelte": "3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-dnd-action": "^0.9.11",
    "svelte-preprocess": "^4.9.8",
    "tippy.js": "^6.3.2",
    "toastify-js": "^1.11.2",
    "tslib": "2.3.1",
    "typescript": "4.3.5"
  }
}
