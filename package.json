{
  "name": "aerie-ui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=10.9.0 <13.0.0",
    "yarn": ">=1.17.3 <1.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.jpl.nasa.gov/MPS/aerie-ui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/pre-commit.js"
    }
  },
  "scripts": {
    "build": "ng build",
    "changelog": "generate-changelog -u https://github.jpl.nasa.gov/MPS/aerie-ui -t 88433ae...904d97a",
    "cloc": "cloc src",
    "format": "prettier --write \"src/**/*.{js,ts,css,html}\" -l",
    "lint": "ng lint --fix",
    "ng": "ng",
    "prebuild": "yarn run version",
    "prestart": "yarn run version",
    "start": "ng serve",
    "test": "ng test --code-coverage --watch=false --browsers FirefoxHeadless",
    "test:watch": "ng test --browsers FirefoxHeadless",
    "version": "node ./scripts/git-version.js"
  },
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~9.0.0-rc.14",
    "@angular/compiler": "~9.0.0-rc.14",
    "@angular/core": "~9.0.0-rc.14",
    "@angular/forms": "~9.0.0-rc.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.0.0-rc.14",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.14",
    "@angular/router": "~9.0.0-rc.14",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "angular-split": "^3.0.2",
    "d3": "^5.15.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.15",
    "ngx-toastr": "^11.3.2",
    "rxjs": "~6.5.4",
    "subsink": "^1.0.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.12",
    "@angular/cli": "~9.0.0-rc.12",
    "@angular/compiler-cli": "~9.0.0-rc.14",
    "@angular/language-service": "~9.0.0-rc.14",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "cloc": "^2.5.1",
    "codelyzer": "^5.2.1",
    "generate-changelog": "^1.8.0",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-firefox-launcher": "~1.2.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.6.4"
  }
}
