{
  "name": "aerie-ui",
  "version": "0.5.0",
  "private": true,
  "engines": {
    "node": ">=10.9.0 <13.0.0",
    "yarn": ">=1.17.3 <2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.jpl.nasa.gov/MPS/aerie-ui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/pre-commit.js"
    }
  },
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "changelog": "generate-changelog -u https://github.jpl.nasa.gov/MPS/aerie-ui -t 9028658...ba4709e",
    "cloc": "cloc src",
    "format": "prettier --write \"src/**/*.{js,ts,css,html}\" -l",
    "lint": "ng lint --fix",
    "ng": "ng",
    "prebuild": "yarn run version",
    "prestart": "yarn run version",
    "pretest": "yarn run version",
    "start": "ng serve",
    "test": "ng test --code-coverage --watch=false --browsers FirefoxHeadless",
    "test:watch": "ng test --code-coverage --watch=true --browsers Firefox",
    "version": "node ./scripts/git-version.js"
  },
  "dependencies": {
    "@gov.nasa.jpl.aerie/time": "1.0.0",
    "@angular/animations": "10.1.4",
    "@angular/cdk": "10.2.3",
    "@angular/common": "10.1.4",
    "@angular/compiler": "10.1.4",
    "@angular/core": "10.1.4",
    "@angular/forms": "10.1.4",
    "@angular/material": "10.2.3",
    "@angular/platform-browser": "10.1.4",
    "@angular/platform-browser-dynamic": "10.1.4",
    "@angular/router": "10.1.4",
    "@apollo/client": "3.2.1",
    "@ctrl/ngx-codemirror": "4.0.1",
    "@ngrx/effects": "10.0.0",
    "@ngrx/router-store": "10.0.0",
    "@ngrx/store": "10.0.0",
    "@ngrx/store-devtools": "10.0.0",
    "angular-split": "4.0.0",
    "apollo-angular": "2.0.4",
    "codemirror": "5.58.1",
    "d3-axis": "2.0.0",
    "d3-brush": "2.1.0",
    "d3-drag": "2.0.0",
    "d3-scale": "3.2.3",
    "d3-selection": "2.0.0",
    "d3-shape": "2.0.0",
    "graphql": "15.3.0",
    "hammerjs": "2.0.8",
    "lodash-es": "4.17.15",
    "ngx-toastr": "13.0.0",
    "rxjs": "6.6.3",
    "subsink": "1.0.1",
    "tslib": "2.0.1",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.3",
    "@angular/cli": "10.1.3",
    "@angular/compiler-cli": "10.1.4",
    "@angular/language-service": "10.1.4",
    "@types/d3-axis": "2.0.0",
    "@types/d3-brush": "2.1.0",
    "@types/d3-drag": "2.0.0",
    "@types/d3-scale": "2.2.3",
    "@types/d3-selection": "2.0.0",
    "@types/d3-shape": "2.0.0",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/lodash-es": "4.17.3",
    "@types/node": "14.11.2",
    "cloc": "2.7.0",
    "codelyzer": "6.0.1",
    "generate-changelog": "1.8.0",
    "husky": "4.3.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-junit-reporter": "2.0.1",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  }
}
