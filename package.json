{
  "name": "aerie-ui",
  "version": "0.6.1",
  "private": true,
  "engines": {
    "node": ">=10.9.0 <13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.jpl.nasa.gov/MPS/aerie-ui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/pre-commit.js"
    }
  },
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "cloc": "cloc src",
    "format": "prettier --write \"src/**/*.{js,ts,css,html}\" -l",
    "lint": "ng lint",
    "ng": "ng",
    "prebuild": "npm run version",
    "prestart": "npm run version",
    "pretest": "npm run version",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "test": "ng test --watch=false --browsers FirefoxHeadless",
    "test:watch": "ng test --watch=true --browsers Firefox",
    "version": "node ./scripts/git-version.js"
  },
