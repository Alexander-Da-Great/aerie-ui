{
  "name": "aerie-ui",
  "version": "0.4.0",
  "private": true,
  "engines": {
    "node": ">=10.9.0 <13.0.0",
    "yarn": ">=1.17.3 <2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.jpl.nasa.gov/MPS/aerie-ui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/pre-commit.js"
    }
  },
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "changelog": "generate-changelog -u https://github.jpl.nasa.gov/MPS/aerie-ui -t 73759fa...5442b98",
    "cloc": "cloc src",
    "format": "prettier --write \"src/**/*.{js,ts,css,html}\" -l",
    "lint": "ng lint --fix",
    "ng": "ng",
    "prebuild": "yarn run version",
    "prestart": "yarn run version",
    "pretest": "yarn run version",
    "start": "ng serve",
    "test": "ng test --code-coverage --watch=false --browsers FirefoxHeadless",
    "test:watch": "ng test --code-coverage --watch=true --browsers Firefox",
    "version": "node ./scripts/git-version.js"
  },
  "dependencies": {
    "@gov.nasa.jpl.aerie/time": "~1.0.0",
    "@angular/animations": "~10.1.1",
    "@angular/cdk": "~10.2.0",
    "@angular/common": "~10.1.1",
    "@angular/compiler": "~10.1.1",
    "@angular/core": "~10.1.1",
    "@angular/forms": "~10.1.1",
    "@angular/material": "~10.2.0",
    "@angular/platform-browser": "~10.1.1",
    "@angular/platform-browser-dynamic": "~10.1.1",
    "@angular/router": "~10.1.1",
    "@ctrl/ngx-codemirror": "~4.0.1",
    "@ngrx/effects": "~10.0.0",
    "@ngrx/router-store": "~10.0.0",
    "@ngrx/store": "~10.0.0",
    "@ngrx/store-devtools": "~10.0.0",
    "angular-split": "~4.0.0",
    "apollo-angular": "~1.10.0",
    "apollo-angular-link-http": "~1.11.0",
    "apollo-cache-inmemory": "~1.6.6",
    "apollo-client": "~2.6.10",
    "apollo-link": "~1.2.14",
    "codemirror": "~5.57.0",
    "d3": "~5.16.0",
    "graphql": "~15.3.0",
    "graphql-tag": "~2.11.0",
    "hammerjs": "~2.0.8",
    "lodash-es": "~4.17.15",
    "ngx-toastr": "~13.0.0",
    "rxjs": "~6.6.3",
    "subsink": "~1.0.1",
    "tslib": "~2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.1",
    "@angular/cli": "~10.1.1",
    "@angular/compiler-cli": "~10.1.1",
    "@angular/language-service": "~10.1.1",
    "@types/d3": "~5.7.2",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash-es": "~4.17.3",
    "@types/node": "~14.10.1",
    "cloc": "~2.6.0",
    "codelyzer": "~6.0.0",
    "generate-changelog": "~1.8.0",
    "husky": "~4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.2.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-firefox-launcher": "~1.3.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.5.4",
    "karma-junit-reporter": "~2.0.1",
    "prettier": "~2.1.1",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.2"
  }
}
